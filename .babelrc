{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": [
          "transform-async-to-generator",
          "transform-regenerator"
        ],
        "useBuiltIns": "entry",
        "corejs": "3"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-async-to-generator",
    "react-hot-loader/babel"
  ]
}
